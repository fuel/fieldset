{
	"name": "fuelphp/fieldset",
	"description": "Form generation and building",
	"keywords": ["fieldset", "form", "form builder"],
	"homepage": "https://github.com/fuelphp/fieldset",
	"license": "MIT",
	"authors": [
		{
			"name": "FuelPHP Development Team",
			"email": "team@fuelphp.com"
		}
	],
	"require": {
		"php": ">=5.4",
		"fuelphp/common": "~2.0"
	},
	"require-dev": {
		"codeception/codeception": "~2.0",
		"codeception/mockery-module": "dev-master"
	},
	"suggest": {
		"fuelphp/validation": "Allows forms to be validated"
	},
	"autoload": {
		"psr-4": {
			"Fuel\\Fieldset\\": "src/"
		}
	},
	"extra": {
		"branch-alias": {
			"dev-master": "2.0-dev"
		}
	}
}
